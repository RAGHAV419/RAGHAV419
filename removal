class Solution {
    public int removeElement(int[] nums, int val) {
       ArrayList <Integer>ll=new ArrayList<>();
		for(int i=0;i<nums.length;i++) {
			if(nums[i]!=val) {
				ll.add(nums[i]);
			}
		}
	
		
		for(int j=0;j<ll.size();j++) {
			nums[j]=ll.get(j);
			
		}
		
	
		return ll.size();
    }
}
