public static void main(String[] args) {
		// TODO Auto-generated method stub
		int nums[]= {1,3,5,6};
		
		int target=7;
		
		System.out.println(search(nums,target));

	}
	public static int search(int nums[],int target) {
		for(int i=0;i<nums.length;i++) {
			
			if(nums[i]>=target) {
			
				return i;
			}
		}
	
		return nums.length;
	}
